<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.business.TbCouponProduct.TbCouponProduct" table="tb_coupon_product" schema="jn_spark_online">
        <id name="id">
            <column name="id" sql-type="varchar(32)" length="32"/>
            <generator class="uuid.hex" />
        </id>
        <property name="type">
            <column name="type" sql-type="varchar(32)" length="32"/>
        </property>
        <property name="contentId">
            <column name="content_id" sql-type="varchar(32)" length="32"/>
        </property>
        <property name="couponId">
            <column name="coupon_id" sql-type="varchar(32)" length="32"/>
        </property>
    </class>
</hibernate-mapping>