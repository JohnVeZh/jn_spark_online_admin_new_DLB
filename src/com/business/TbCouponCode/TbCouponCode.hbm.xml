<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.business.TbCouponCode.TbCouponCode" table="tb_coupon_code" schema="jn_spark_online">
        <id name="id">
            <column name="id" sql-type="varchar(32)" length="32"/>
        </id>
        <property name="code">
            <column name="code" sql-type="varchar(32)" length="32"/>
        </property>
        <property name="couponId">
            <column name="coupon_id" sql-type="varchar(32)" length="32"/>
        </property>
        <property name="useNum">
            <column name="use_num" sql-type="int(11)"/>
        </property>
        <property name="createTime">
            <column name="create_time" sql-type="datetime"/>
        </property>
    </class>
</hibernate-mapping>