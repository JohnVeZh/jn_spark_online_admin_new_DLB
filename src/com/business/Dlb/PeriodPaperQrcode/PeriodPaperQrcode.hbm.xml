<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

     <class name="com.business.Dlb.PeriodPaperQrcode.PeriodPaperQrcode" table="dlb_period_paper_qrcode" schema="jn_spark_online">
        <id name="id">
            <column name="id" sql-type="varchar(32)" length="32"/>
            <generator class="uuid.hex" />
        </id>
        <property name="section">
            <column name="section" sql-type="varchar(32)" length="32" not-null="true"/>
        </property>
        <property name="period">
            <column name="period" sql-type="tinyint(4)" length="4" not-null="true"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(128)" not-null="true"/>
        </property>
        <property name="qrcodeContent">
            <column name="qrcode_content" sql-type="varchar(128)" not-null="true"/>
        </property>
        <property name="qrcodeUrl">
            <column name="qrcode_url" sql-type="varchar(256)" not-null="true"/>
        </property>
        <property name="userId">
            <column name="user_id" sql-type="varchar(32)" not-null="true"/>
        </property>
        <property name="createDate">
            <column name="create_date" sql-type="datetime" not-null="true"/>
        </property>
        <property name="delFlag">
            <column name="del_flag" sql-type="tinyint(4)" not-null="true"/>
        </property>
        <property name="username"></property>
    </class>
</hibernate-mapping>